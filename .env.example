# Supabase Configuration
# Obtenha essas credenciais em: https://supabase.com/dashboard/project/YOUR_PROJECT/settings/api

# URL do projeto Supabase (obrigatório)
NEXT_PUBLIC_SUPABASE_URL=https://your-project-ref.supabase.co

# Chave anônima pública do Supabase (obrigatório)
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here

# Chave de serviço do Supabase (obrigatório para operações administrativas)
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# URL de conexão direta ao banco de dados PostgreSQL (opcional)
DATABASE_URL=postgresql://postgres.your-project-ref:your-password@aws-0-region.pooler.supabase.com:6543/postgres

# Modo de desenvolvimento (opcional)
APP_MODO_MOCK=true
